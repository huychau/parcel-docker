{
  "name": "parcel-docker",
  "version": "1.0.0",
  "description": "Run Parcel inside Docker",
  "main": "index.js",
  "scripts": {
    "start": "rm -rf dist/ && parcel src/*.pug --no-cache --no-source-maps",
    "build": "rm -rf public/ && parcel build src/*.pug --no-cache --no-source-maps --no-minify --experimental-scope-hoisting --out-dir public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huychau/parcel-docker.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/huychau/parcel-docker/issues"
  },
  "homepage": "https://github.com/huychau/parcel-docker#readme",
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "cssnano": "^4.1.10",
    "pug": "^2.0.3",
    "sass": "^1.19.0"
  }
}
